--Properties:

ScreenGui.Parent = game.CoreGui

main.Name = "main"
main.Parent = ScreenGui
main.BackgroundColor3 = Color3.fromRGB(72, 72, 72)
main.Position = UDim2.new(0.801935852, 0, 0.345098048, 0)
main.Size = UDim2.new(0, 246, 0, 150)
main.Active = true
main.Draggable = true

Titel.Name = "Titel"
Titel.Parent = main
Titel.BackgroundColor3 = Color3.fromRGB(62, 62, 62)
Titel.Size = UDim2.new(0, 222, 0, 11)
Titel.Font = Enum.Font.RobotoCondensed
